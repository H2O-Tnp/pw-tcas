{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "#/*": ["./*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "ui/SessionWrapper.tsx",
    "ui/global-nav.js",
    "ui/profile-logo.tsx"
, "ui/form copy.js", "ui/form.js", "app/student/form/score/studentForm.fsdftsx", "app/student/form/score/page.tsxdd", "oldauth.ts", "auth.config.ts.old"  ],
  "exclude": ["node_modules"]
}
